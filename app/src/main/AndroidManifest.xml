<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eztest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EZTest">
        <activity
            android:name=".activities.ScoreActivity"
            android:exported="true" />
        <activity
            android:name=".adapters.quizAdapter"
            android:exported="true"
            android:label="@string/title_activity_quiz_adapter"
            android:theme="@style/Theme.EZTest.NoActionBar" />
        <activity
            android:name=".activities.TeacherMain"
            android:exported="true" />
        <activity
            android:name=".activities.StudentMain"
            android:exported="true" />
        <activity
            android:name=".activities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>